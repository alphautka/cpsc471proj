<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ark.myapplication.MainMenuActivity">

    <Button
        android:id="@+id/liquorButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Liquor"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/nonLiquorButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Non-Liquor"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="78dp" />

    <Button
        android:id="@+id/miscButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Miscellaneous"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="151dp" />

    <LinearLayout
        android:layout_width="377dp"
        android:layout_height="568dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"></LinearLayout>
</android.support.constraint.ConstraintLayout>

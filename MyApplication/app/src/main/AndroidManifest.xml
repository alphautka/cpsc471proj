<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ark.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".CatalogActivity" />
        <activity android:name=".CatalogListActivity" />
        <activity android:name=".EmployeeView" />
        <activity android:name=".CatalogView" />
        <activity android:name=".DetailedItemView" />
        <activity android:name=".OrderForm" />
        <activity android:name=".OrderItem"></activity>
    </application>

</manifest>